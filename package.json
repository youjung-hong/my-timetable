{
  "name": "my-timetable",
  "version": "0.0.4",
  "private": true,
  "license": "MIT",
  "main": "./dist/main.js",
  "types": "./types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:youjung-hong/my-timetable.git"
  },
  "keywords": [
    "planner",
    "timetable",
    "time tracker",
    "goal tracker",
    "10 minutes planner"
  ],
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "webpack --mode=development",
    "types": "dts-bundle-generator --umd-module-name MyTimetable ./src/index.ts -o ./types/index.d.ts",
    "transfile": "npx tsc ./src/**/*.ts$",
    "serve": "webpack-dev-server --mode=development",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build-storybook-docs": "rm -rf docs && yarn run build-storybook && mv storybook-static docs"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/html": "^6.5.9",
    "@storybook/jest": "^0.0.10",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@types/jest": "^28.1.3",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "dts-bundle-generator": "^6.11.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.2",
    "style-loader": "^3.3.1",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  },
  "dependencies": {}
}
